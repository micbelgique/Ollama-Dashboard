{
  "name": "ollama-desktop",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "clean": "pnpm dlx rimraf .next node_modules",
    "analyze": "ANALYZE=true pnpm build",
    "update": "pnpm update --interactive",
    "deps:check": "pnpm dlx depcheck --ignores=\"@types/*,eslint*,next\"",
    "deps:dedupe": "pnpm dedupe",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.8.3",
    "next": "15.2.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.0",
    "@types/node": "^20.17.24",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "eslint": "^9.22.0",
    "eslint-config-next": "15.2.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1"
  }
}
